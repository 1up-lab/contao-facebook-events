{
  "name":"oneup/contao-facebook-events",
  "description":"This Contao Open Source CMS extension gives you the ability to fetch events from facebook into your calender.",
  "keywords":["contao", "events", "facebook", "extension", "module", "calendar"],
  "type":"contao-module",
  "homepage":"http://1up.io",
  "license":"MIT",
  "authors":[
    {
      "name":"David Greminger",
      "email":"dg@1up.io",
      "homepage":"http://1up.io",
      "role":"Developer"
    },
    {
      "name":"Jim Schmid",
      "email":"js@1up.io",
      "homepage":"http://1up.io",
      "role":"Developer"
    }
  ],
  "support":{
    "email":"hello@1up.io",
    "issues":"https://github.com/1up-lab/contao-facebook-events/issues",
    "irc":"irc://irc.freenode.org/contao.dev",
    "source":"https://github.com/1up-lab/contao-facebook-events"
  },
  "require":{
    "php":"^5.4|^7.0",
    "contao/core":">=3.2,<5.0-dev",
    "contao-community-alliance/composer-plugin": "~2.0",
    "facebook/graph-sdk": "~4.0",
    "guzzlehttp/guzzle": "~5.0"
  },
  "autoload": {
    "psr-4": {
      "Oneup\\Contao\\": "src"
    }
  },
  "extra": {
    "contao": {
      "sources": {
        "contao": "system/modules/facebook-events"
      }
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
